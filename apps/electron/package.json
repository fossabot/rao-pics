{
  "name": "@acme/electron",
  "productName": "Rao Pics",
  "version": "0.7.13",
  "description": "Visit Photos on Any Device.",
  "homepage": "https://rao.pics",
  "private": true,
  "main": "main/dist/index.cjs",
  "type": "module",
  "author": {
    "name": "meetqy",
    "email": "meetqy@qq.com"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm run watch:desktop",
    "build": "pnpm '/^build:.*/'",
    "clean": "git clean -xdf dist node_modules .turbo",
    "release": "git clean -xdf ./dist && pnpm '/^release:.*/'",
    "release:mac": "git clean -xdf ./dist/mac && ts-node electron-builder/mac.ts",
    "release:win": "git clean -xdf ./dist/win && ts-node electron-builder/win.ts",
    "release:linux-openssl-1.1.x": "git clean -xdf ./dist/linux-openssl-1.1.x && cross-env OPENSSL_VERSION=openssl-1.1.x ts-node electron-builder/linux.ts",
    "release:linux-openssl-3.0.x": "git clean -xdf ./dist/linux-openssl-3.0.x && cross-env OPENSSL_VERSION=openssl-3.0.x ts-node electron-builder/linux.ts",
    "watch:web": "ts-node scripts/watchWeb.ts",
    "watch:desktop": "ts-node scripts/watchDesktop.ts",
    "build:main": " vite build main",
    "build:preload": "vite build preload",
    "build:renderer": "vite build renderer",
    "sync:version": "node scripts/sync-version.mjs",
    "vendors:update": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.mjs",
    "postinstall": "npm run vendors:update",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@acme/eslint-config": "^0.1.0",
    "@acme/tailwind-config": "^0.1.0",
    "@sentry/vite-plugin": "^2.5.0",
    "@types/express": "^4.17.17",
    "@types/ip": "^1.1.0",
    "@types/node": "^18.16.5",
    "@types/node-fetch": "^2.6.4",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^2.1.0",
    "cross-env": "^7.0.3",
    "electron": "20.1.4",
    "electron-builder": "23.3.3",
    "eslint": "8.42.0",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^3.1.3"
  },
  "dependencies": {
    "@acme/api": "^0.1.0",
    "@acme/constant": "^0.1.0",
    "@acme/curd": "^0.1.0",
    "@acme/db": "^0.1.0",
    "@acme/eagle": "^0.1.0",
    "@acme/lang": "^0.1.0",
    "@acme/watch": "^0.1.0",
    "@sentry/electron": "^4.8.0",
    "@sentry/react": "^7.61.1",
    "@tanstack/react-query": "^4.29.5",
    "@trpc/client": "^10.23.0",
    "@trpc/react-query": "^10.25.0",
    "@trpc/server": "^10.23.0",
    "electron-trpc": "^0.5.2",
    "express": "^4.18.2",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "get-port-please": "^3.0.1",
    "ip": "^1.1.8",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.12.3",
    "zod": "^3.21.4"
  }
}
