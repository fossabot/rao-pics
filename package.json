{
  "name": "eagleuse",
  "version": "0.1.13",
  "description": "使用《Eagle App》作为后台管理系统，快速构建 WEB 图片站、自建图床。",
  "packageManager": "pnpm@7.1.0",
  "keywords": [
    "eagle",
    "next.js",
    "prisma",
    "sqlite",
    "typescript",
    "antd",
    "react"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetqy/eagleuse.git"
  },
  "bugs": {
    "url": "https://github.com/meetqy/eagleuse/issues"
  },
  "homepage": "https://github.com/meetqy/eagleuse#readme",
  "author": "meetqy",
  "scripts": {
    "dev": "pnpm run setup & next",
    "build": "npm run db:generate && next build",
    "start": "next start --port 9623",
    "setup": "ts-node setup.ts",
    "db:init": "prisma migrate dev --name init --skip-seed --schema ./node_modules/@eagleuse/prisma-client/prisma/schema.prisma",
    "db:preview": "prisma studio --schema ./node_modules/@eagleuse/prisma-client/prisma/schema.prisma",
    "lint": "next lint",
    "fix": "npx prettier --write .",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@eagleuse/plugin-api": "^1.1.2",
    "@eagleuse/plugin-nsfw": "^1.1.0",
    "@eagleuse/transform-eagle": "^1.1.12",
    "@prisma/client": "^4.10.1",
    "@types/justified-layout": "^4.1.0",
    "ahooks": "^3.7.4",
    "antd": "^5.2.1",
    "chokidar": "^3.5.3",
    "encoding": "^0.1.13",
    "express": "^4.18.2",
    "git-cz": "^4.9.0",
    "justified-layout": "^4.1.0",
    "lodash": "^4.17.21",
    "next": "13.1.1",
    "pino": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.4",
    "recoil": "^0.7.6",
    "standard-version": "^9.5.0"
  },
  "devDependencies": {
    "@eagleuse/prisma-client": "^1.0.2",
    "@tensorflow/tfjs-node": "^4.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.11.12",
    "@types/react": "18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.6.0",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "nsfwjs": "^2.4.2",
    "prisma": "^4.10.1",
    "progress": "^2.0.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "4.9.4"
  },
  "prisma": {
    "seed": "ts-node prisma-seed.ts"
  },
  "lint-staged": {
    "**/*.(ts|tsx)": "eslint --fix --max-warnings 0",
    "**/*": "prettier --write --ignore-unknown"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
